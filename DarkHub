    local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
    local Window = OrionLib:MakeWindow({Name = "Dark Hub", HidePremium = false, IntroText = 'Dark Hub', SaveConfig = true, ConfigFolder = "DarkConfig"})
    
    --AutoFarm
    local Tab = Window:MakeTab({
        Name = "AutoFarm",
        Icon = "rbxassetid://4483345998",
        PremiumOnly = false
    })
    
    --AutoClicker
    local Section = Tab:AddSection({
        Name = "AutoClicker"
    })
    
    --Botão

Tab:AddButton({
	Name = "Auto Clicker",
	Callback = function()
      		print("Botão pressionado")
  	end    
})
    
    
    OrionLib:Init()
    
